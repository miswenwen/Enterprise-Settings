<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.honeywell.ezservicetest"
    android:sharedUserId="android.uid.system">
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.honeywell.ezservicetest.EzServiceTestActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.honeywell.ezservicetest.scan.ScanResultKeySetActivity"/>
        <activity android:name="com.honeywell.ezservicetest.general.RuntimePermissionsSetActivity"/>
        <activity android:name="com.honeywell.ezservicetest.scan.ScanSettingsTestActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>